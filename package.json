{
  "name": "tstack",
  "version": "0.1.2-alpha.13",
  "description": "A fully TypeScript based stack library to be used used with Angular & Express",
  "author": "Dustin M. Eastway <Dustin.Eastway@gmail.com>",
  "repository": "github:DustinMEastway/tstack",
  "homepage": "https://github.com/DustinMEastway/dme-modules#readme",
  "license": "MIT",
  "scripts": {
    "build": "rm -r ./dist; npm run buildCore; npm run buildClient; npm run buildServer",
    "buildClient": "ng build client; npm run copyClientAssets",
    "buildCore": "ng build core",
    "buildServer": "ng build server",
    "copyClientAssets": "cp -R ./projects/client/src/lib/assets/. ./dist/@tstack/client",
    "docs": "node ./tools/docs",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test --watch=false"
  },
  "private": true,
  "bugs": {
    "url": "https://github.com/DustinMEastway/dme-modules/issues"
  },
  "dependencies": {
    "@angular/animations": "^7.2.6",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "^7.2.6",
    "@angular/compiler": "^7.2.6",
    "@angular/core": "^7.2.6",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "^7.2.6",
    "@angular/material": "^7.3.3",
    "@angular/platform-browser": "^7.2.6",
    "@angular/platform-browser-dynamic": "^7.2.6",
    "@angular/router": "^7.2.6",
    "core-js": "^2.5.4",
    "rxjs": "^6.4.0",
    "tsickle": "^0.34.3",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular-devkit/build-ng-packagr": "^0.13.8",
    "@angular/cli": "^7.3.8",
    "@angular/compiler-cli": "^7.2.6",
    "@angular/language-service": "^7.2.6",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^4.5.0",
    "dgeni": "^0.4.12",
    "dgeni-packages": "^0.27.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.7.1",
    "protractor": "^5.4.2",
    "ts-node": "~5.0.1",
    "tslib": "^1.9.0",
    "tslint": "^5.13.0",
    "typescript": "^3.2.4"
  },
  "keywords": [
    "TypeScript",
    "Angular",
    "Express",
    "Node.js",
    "Web Development",
    "Stack"
  ]
}
