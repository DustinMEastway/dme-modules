{
  "name": "tstack",
  "version": "1.0.0",
  "description": "A fully TypeScript based stack library to be used used with Angular & Express",
  "author": "Dustin M. Eastway <Dustin.Eastway@gmail.com>",
  "repository": "github:DustinMEastway/tstack",
  "homepage": "https://github.com/DustinMEastway/dme-modules#readme",
  "license": "MIT",
  "scripts": {
    "build": "rm -r ./dist; npm run buildCore; npm run buildClient; npm run buildServer; npm run buildDocs;",
    "buildClient": "ng build client; npm run copyClientAssets",
    "buildDocs": "node ./tools/docs; ng build --prod",
    "buildDocsWatch": "nodemon ./tools/docs --watch ./tools --watch ./projects --ext ts,js,md",
    "buildCore": "ng build core",
    "buildServer": "ng build server",
    "copyClientAssets": "cp -R ./projects/client/src/lib/assets/. ./dist/@tstack/client",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "start": "npm run buildDocsWatch & ng serve",
    "test": "ng test --watch=false"
  },
  "private": true,
  "bugs": {
    "url": "https://github.com/DustinMEastway/dme-modules/issues"
  },
  "dependencies": {
    "@angular/animations": "^8.0.3",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "^8.0.3",
    "@angular/compiler": "^8.0.3",
    "@angular/core": "^8.0.3",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "^8.0.3",
    "@angular/material": "^8.0.1",
    "@angular/platform-browser": "^8.0.3",
    "@angular/platform-browser-dynamic": "^8.0.3",
    "@angular/router": "^8.0.3",
    "core-js": "^2.5.4",
    "marked": "^0.6.2",
    "rxjs": "^6.5.2",
    "tsickle": "^0.35.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular-devkit/build-ng-packagr": "~0.800.0",
    "@angular/cli": "^8.0.4",
    "@angular/compiler-cli": "^8.0.3",
    "@angular/language-service": "^8.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/marked": "^0.6.5",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "dgeni": "^0.4.12",
    "dgeni-packages": "^0.27.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^5.1.0",
    "nodemon": "^1.19.0",
    "protractor": "^5.4.2",
    "ts-node": "~5.0.1",
    "tslib": "^1.10.0",
    "tslint": "^5.13.0",
    "typescript": "^3.4.5"
  },
  "keywords": [
    "TypeScript",
    "Angular",
    "Express",
    "Node.js",
    "Web Development",
    "Stack"
  ]
}
