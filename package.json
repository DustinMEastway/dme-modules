{
  "name": "tstack",
  "version": "2.0.0-beta.0",
  "description": "A fully TypeScript based stack library to be used used with Angular & Express",
  "author": "Dustin M. Eastway <Dustin.Eastway@gmail.com>",
  "repository": "github:DustinMEastway/tstack",
  "homepage": "https://dustinmeastway.github.io/tstack",
  "license": "MIT",
  "scripts": {
    "build": "npm run build/core && npm run build/angular && npm run build/tstack",
    "build/angular": "npm --prefix projects/angular run build",
    "build/core": "npm --prefix projects/core run build",
    "build/tstack": "ng build tstack",
    "lint": "eslint \"**/*.ts\"",
    "start": "ng serve --open",
    "test": "ng test tstack"
  },
  "private": true,
  "bugs": {
    "url": "https://github.com/DustinMEastway/tstack/issues"
  },
  "dependencies": {
    "@angular/animations": "~12.0.0",
    "@angular/common": "~12.0.0",
    "@angular/compiler": "~12.0.0",
    "@angular/core": "~12.0.0",
    "@angular/forms": "~12.0.0",
    "@angular/platform-browser": "~12.0.0",
    "@angular/platform-browser-dynamic": "~12.0.0",
    "@angular/router": "~12.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^11.2.0",
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular-eslint/eslint-plugin": "^12.0.0",
    "@angular-eslint/eslint-plugin-template": "^12.0.0",
    "@angular-eslint/template-parser": "^12.0.0",
    "@angular/cli": "~12.0.0",
    "@angular/compiler-cli": "~12.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.26.0",
    "jest": "^26.6.3",
    "ng-packagr": "^12.0.0",
    "typescript": "~4.2.3"
  },
  "keywords": [
    "TypeScript",
    "Angular",
    "Express",
    "Node.js",
    "Web Development",
    "Stack"
  ]
}
