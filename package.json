{
  "name": "tstack",
  "version": "0.1.2-alpha.10",
  "description": "A fully TypeScript based stack library to be used used with Angular & Express",
  "author": "Dustin M. Eastway <Dustin.Eastway@gmail.com>",
  "repository": "github:DustinMEastway/tstack",
  "homepage": "https://github.com/DustinMEastway/dme-modules#readme",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "copyClientAssets": "cp -R ./projects/client/src/lib/assets/. ./dist/@tstack/client",
    "build": "rm -r ./dist; npm run buildCore; npm run buildClient; npm run buildServer",
    "buildClient": "ng build --prod client && npm run copyClientAssets",
    "buildCore": "ng build --prod core",
    "buildServer": "ng build --prod server",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "bugs": {
    "url": "https://github.com/DustinMEastway/dme-modules/issues"
  },
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/cdk": "^6.3.2",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/flex-layout": "^6.0.0-beta.17",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "^6.3.2",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.0.3",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular-devkit/build-angular": "~0.6.8",
    "ng-packagr": "^3.0.0-rc.2",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "typescript": "~2.7.2",
    "@angular/cli": "~6.0.8",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1"
  },
  "keywords": [
    "TypeScript",
    "Angular",
    "Express",
    "Node.js",
    "Web Development",
    "Stack"
  ]
}
